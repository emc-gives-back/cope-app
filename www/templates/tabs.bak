<!--
Create tabs with an icon and label, using the tabs-positive style.
Each tab's child <ion-nav-view> directive will have its own
navigation history that also transitions its views in and out.
-->
<ion-tabs class="tabs-icon-top tabs-color-active-positive">

  <!-- Dashboard Tab -->
  <ion-tab title="Dashboard" icon-off="ion-ios-home-outline" icon-on="ion-ios-home" href="#/tab/dash">
    <ion-nav-view name="tab-dash"></ion-nav-view>
  </ion-tab>

  <!-- Health Tab -->
  <ion-tab title="Health" icon-off="ion-ios-medkit-outline" icon-on="ion-ios-medkit" href="#/tab/chats">
    <ion-nav-view name="tab-health"></ion-nav-view>
  </ion-tab>

  <!-- Health Tab -->
  <ion-tab title="Health - Washing" icon-off="ion-ios-medkit-outline" icon-on="ion-ios-medkit" href="#/tab/chats">
    <ion-nav-view name="tab-health-washing"></ion-nav-view>
  </ion-tab>

</ion-tabs>
